@using Harmony.Application.DTO.Search
<MudStack Class="SearchStack">
    <MudAutocomplete T="SearchableCard" @bind-Value="_selectedCard" Margin="Margin.Dense"
                     Clearable="true" Placeholder="Search.."
                     DebounceInterval="1500"
                     ShowProgressIndicator="true" ProgressIndicatorColor="Color.Default"
                     SearchFuncWithCancel="@Search" Variant="Variant.Outlined">
        <ItemTemplate Context="card">
            <MudText>@card.Title</MudText>
            </ItemTemplate>
        </MudAutocomplete>
    </MudStack>
